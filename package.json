    "icon": "icon.png",
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
      },
    "homepage": "https://github.com/shahriarshafin/bootstrap-5-snippets/blob/master/README.md",
    "keywords": [
        "snippets",
        "react",
        "redux",
        "react-native",
        "javascript",
